#: napari/layers/shapes/_shapes_models/_polgyon_base.py:71
msgid "Shape needs at least two vertices, {number} provided."
msgstr "Το σχήμα χρειάζεται τουλάχιστον δύο κορυφές και {number} παρέχονται."

#: napari/layers/shapes/_shapes_models/ellipse.py:76
msgid "Data shape does not match a ellipse. Ellipse expects four corner vertices, {number} provided."
msgstr "Το σχήμα δεδομένων δεν ταιριάζει με μια έλλειψη. Η έλλειψη αναμένει τέσσερις γωνιακές κορυφές {number}."

#: napari/layers/shapes/_shapes_models/line.py:61
msgid "Data shape does not match a line. A line expects two end vertices, {number} provided."
msgstr "Το σχήμα δεδομένων δεν ταιριάζει με μια γραμμή. Μια γραμμή αναμένει δύο τελικές κορυφές {number}."

#: napari/layers/shapes/_shapes_models/rectangle.py:68
msgid "Data shape does not match a rectangle. Rectangle expects four corner vertices, {number} provided."
msgstr "Το σχήμα δεδομένων δεν ταιριάζει με ένα ορθογώνιο. Το ορθογώνιο αναμένει τέσσερις γωνιακές κορυφές {number}."

#: napari/layers/shapes/_shapes_models/shape.py:346
msgid "Axis not recognized, must be one of \"{{0, 1}}\""
msgstr "Ο άξονας δεν αναγνωρίζεται, πρέπει να είναι ένας από τους \"{{0, 1}}\""

#: napari/layers/shapes/_shapes_models/shape.py:400
msgid "mask shape length must either be 2 or the same as the dimensionality of the shape, expected {expected} got {received}."
msgstr "το μήκος του σχήματος μάσκας πρέπει είτε να είναι 2 είτε το ίδιο με τη διάσταση του σχήματος, αναμένεται ότι {expected} πήρε {received}."

#: napari/layers/image/experimental/_octree_slice.py:92
msgid "Octree level {index} is not in range(0, {num_levels})"
msgstr "Το επίπεδο οκτόνου {index} δεν είναι εύρος(0, {num_levels})"

#: napari/layers/image/experimental/_octree_slice.py:137
msgid "Invalid octree level {index}"
msgstr "Μη έγκυρο επίπεδο octree {index}"

#: napari/layers/image/experimental/octree.py:66
msgid "Data of shape {shape} resulted no octree levels?"
msgstr "Τα δεδομένα του σχήματος {shape} δεν προέκυψαν επίπεδα octretrey?"

#: napari/layers/image/experimental/octree.py:116
msgid "Level {level_index} is not in range(0, {top})"
msgstr "Το επίπεδο {level_index} δεν είναι σε εύρος(0, {top})"

#: napari/layers/image/experimental/octree.py:403
msgid "Multiscale data has downsampling ratio of {ratio}, expected 2."
msgstr "Multiscale data has downsampling ratio {ratio}, expected 2."

#: napari/layers/image/experimental/octree_image.py:223
msgid "Octree level {level} is negative."
msgstr "Το επίπεδο οκτώδων {level} είναι αρνητικό."

#: napari/layers/labels/_labels_constants.py:71
msgid "circle"
msgstr "κύκλος"

#: napari/layers/labels/_labels_constants.py:72 napari/layers/points/_points_constants.py:91
msgid "square"
msgstr "τετράγωνο"

#: napari/layers/labels/_labels_constants.py:77
msgid "auto"
msgstr "αυτόματο"

#: napari/layers/labels/_labels_constants.py:78
msgid "direct"
msgstr "άμεση"

#: napari/layers/labels/labels.py:252 napari/layers/labels/labels.py:606
msgid "enter paint or fill mode to edit labels"
msgstr "εισάγετε το χρώμα ή τη λειτουργία πλήρωσης για να επεξεργαστείτε ετικέτες"

#: napari/layers/labels/labels.py:401
msgid "Only integer types are supported for Labels layers, but data contains {data_level_type}."
msgstr "Υποστηρίζονται μόνο ακέραιοι τύποι για στρώματα ετικετών, αλλά τα δεδομένα περιέχουν {data_level_type}."

#: napari/layers/labels/labels.py:427
msgid "the number of items must be equal for all properties"
msgstr "ο αριθμός των στοιχείων πρέπει να είναι ίσος για όλες τις ιδιότητες"

#: napari/layers/labels/labels.py:469
msgid "cannot reduce selected label below 0"
msgstr "δεν μπορεί να μειώσει την επιλεγμένη ετικέτα κάτω από 0"

#: napari/layers/labels/labels.py:507
msgid "Unsupported Color Mode"
msgstr "Μη Υποστηριζόμενη Λειτουργία Χρώματος"

#: napari/layers/labels/labels.py:532 napari/layers/labels/labels.py:549
msgid "The square brush shape is deprecated and will be removed in version 0.4.9. Afterward, only the circle brush shape will be available, and the layer.brush_shape attribute will be removed."
msgstr "Το σχήμα τετράγωνης βούρτσας είναι παρωχημένο και θα αφαιρεθεί στην έκδοση 0.4.9. Στη συνέχεια, μόνο το σχήμα του πινέλου κύκλου θα είναι διαθέσιμο, και το χαρακτηριστικό layer.brush_shape θα αφαιρεθεί."

#: napari/layers/labels/labels.py:610
msgid "hold <space> to pan/zoom, click to pick a label"
msgstr "κρατήστε πατημένο το <space> για εμφάνιση / ζουμ, κάντε κλικ για να επιλέξετε μια ετικέτα"

#: napari/layers/labels/labels.py:623
msgid "hold <space> to pan/zoom, hold <shift> to toggle preserve_labels, hold <control> to fill, hold <alt> to erase, drag to paint a label"
msgstr "κρατήστε πατημένο το <space> σε pan/zoom, κρατήστε πατημένο το <shift> για εναλλαγή του preserve_labels, κρατήστε πατημένο το <control> για να γεμίσετε, κρατήστε πατημένο το <alt> για διαγραφή, σύρετε για να ζωγραφίσετε μια ετικέτα"

#: napari/layers/labels/labels.py:630
msgid "hold <space> to pan/zoom, click to fill a label"
msgstr "κρατήστε πατημένο το <space> για εμφάνιση/μεγέθυνση, κάντε κλικ για να γεμίσετε μια ετικέτα"

#: napari/layers/labels/labels.py:643
msgid "hold <space> to pan/zoom, drag to erase a label"
msgstr "κρατήστε πατημένο το <space> στο πάτημα/ζουμ, σύρετε για να διαγράψετε μια ετικέτα"

#: napari/_qt/layer_controls/qt_labels_controls.py:287 napari/_qt/layer_controls/qt_points_controls.py:225 napari/layers/labels/labels.py:648 napari/layers/points/points.py:1120 napari/layers/shapes/shapes.py:1495
msgid "Mode not recognized"
msgstr "Η λειτουργία δεν αναγνωρίζεται"

#: napari/layers/labels/labels.py:774
msgid "Contours are not displayed during 3D rendering"
msgstr "Οι περιηγήσεις δεν εμφανίζονται κατά την τρισδιάστατη απόδοση"

#: napari/layers/labels/labels.py:1082
msgid "[No Properties]"
msgstr "[Χωρίς Ιδιότητες]"

#: napari/layers/tracks/_track_utils.py:205
msgid "track vertices should be a NxD array"
msgstr "οι κορυφές των κομματιών πρέπει να είναι ένας πίνακας NxD"

#: napari/layers/tracks/_track_utils.py:210
msgid "track vertices should be 4 or 5-dimensional"
msgstr "οι κορυφές των κομματιών πρέπει να είναι 4 ή 5 διαστάσεων"

#: napari/layers/tracks/_track_utils.py:220
msgid "track id must be an integer"
msgstr "το αναγνωριστικό κομματιού πρέπει να είναι ακέραιος αριθμός"

#: napari/layers/tracks/_track_utils.py:225
msgid "track timestamps must be greater than zero"
msgstr "οι χρονικές σφραγίδες των κομματιών πρέπει να είναι μεγαλύτερες από το μηδέν"

#: napari/layers/tracks/_track_utils.py:234
msgid "tracks should be ordered by ID and time"
msgstr "τα κομμάτια πρέπει να παραγγελθούν με ταυτότητα και χρόνο"

#: napari/layers/tracks/_track_utils.py:249
msgid "the number of properties must equal the number of vertices"
msgstr "ο αριθμός των ιδιοτήτων πρέπει να ισούται με τον αριθμό των κορυφών"

#: napari/layers/tracks/_track_utils.py:277
msgid "graph node {node_idx} not found"
msgstr "ο κόμβος γραφήματος {node_idx} δεν βρέθηκε"

#: napari/layers/tracks/_track_utils.py:345
msgid "Property {color_by} not found"
msgstr "Το ακίνητο {color_by} δεν βρέθηκε"

#: napari/layers/tracks/tracks.py:385
msgid "Previous color_by key {key!r} not present in new properties. Falling back to track_id"
msgstr "Προηγούμενο color_by key {key!r} δεν υπάρχει σε νέες ιδιότητες. Επιστρέφοντας στο track_id"

#: napari/layers/tracks/tracks.py:470
msgid "{color_by} is not a valid property key"
msgstr "{color_by} δεν είναι έγκυρο κλειδί ιδιότητας"

#: napari/layers/tracks/tracks.py:489
msgid "Colormap {colormap} not available"
msgstr "Χρωματισμός χάρτη {colormap} δεν είναι διαθέσιμη"

#: napari/layers/surface/surface.py:157
msgid "Surface data tuple must be 2 or 3, specifying verictes, faces, and optionally vertex values, instead got length {length}."
msgstr "Ο σωλήνας δεδομένων επιφάνειας πρέπει να είναι 2 ή 3, καθορίζοντας κάθετες, όψεις και προαιρετικά τιμές vertex, αντ' αυτού πήρε μήκος {length}."

#: napari/layers/surface/surface.py:203
msgid "Surface data tuple must be 2 or 3, specifying vertices, faces, and optionally vertex values, instead got length {data_length}."
msgstr "Ο σωλήνας δεδομένων επιφάνειας πρέπει να είναι 2 ή 3, καθορίζοντας κορυφές, όψεις και προαιρετικά τιμές vertex, αντί αυτού πήρε μήκος {data_length}."

#: napari/layers/surface/surface.py:318
msgid "Assigning multiple values per vertex after slicing is not allowed. All dimensions corresponding to vertex_values must be non-displayed dimensions. Data will not be visible."
msgstr "Η ανάθεση πολλαπλών τιμών ανά κορυφή μετά τον τεμαχισμό δεν επιτρέπεται. Όλες οι διαστάσεις που αντιστοιχούν σε vertex_values πρέπει να είναι μη εμφανιζόμενες διαστάσεις. Τα δεδομένα δεν θα είναι ορατά."

#: napari/layers/shapes/_shape_list.py:152
msgid "{attribute}_color must have shape ({n_shapes}, 4)"
msgstr "{attribute}_color πρέπει να έχει σχήμα ({n_shapes}, 4)"

#: napari/layers/shapes/_shape_list.py:248
msgid "shape must be subclass of Shape"
msgstr "το σχήμα πρέπει να είναι υποκλάση του Shape"

#: napari/layers/shapes/_shape_list.py:490
msgid "{shape_type} must be one of {shape_classes}"
msgstr "{shape_type} πρέπει να είναι ένα από {shape_classes}"

#: napari/layers/shapes/_shapes_utils.py:403
msgid "Data shape does not match expected `[4, D]` shape specifying corners for the rectangle"
msgstr "Το σχήμα δεδομένων δεν ταιριάζει με το αναμενόμενο σχήμα `[4, D]` καθορίζοντας τις γωνίες για το ορθογώνιο"

#: napari/layers/shapes/_shapes_utils.py:498
msgid "Data shape does not match expected `[4, D]` shape specifying corners for the ellipse"
msgstr "Το σχήμα δεδομένων δεν ταιριάζει με το αναμενόμενο σχήμα `[4, D]` καθορίζοντας τις γωνίες για την έλλειψη"

#: napari/layers/shapes/shapes.py:337
msgid "Shape dimensions must be equal to ndim"
msgstr "Οι διαστάσεις σχήματος πρέπει να είναι ίσες με ndim"

#: napari/layers/points/points.py:329 napari/layers/shapes/shapes.py:407
msgid "text should be a string, array, or dict"
msgstr "το κείμενο πρέπει να είναι συμβολοσειρά, διάταξη ή dict"

#: napari/layers/shapes/shapes.py:452 napari/layers/shapes/shapes.py:1454
msgid "enter a selection mode to edit shape properties"
msgstr "εισάγετε μια λειτουργία επιλογής για να επεξεργαστείτε τις ιδιότητες σχήματος"

#: napari/layers/points/points.py:505 napari/layers/shapes/shapes.py:626
msgid "property used for face_color dropped"
msgstr "ιδιότητα που χρησιμοποιήθηκε για face_color έπεσε"

#: napari/layers/points/points.py:524 napari/layers/shapes/shapes.py:638 napari/layers/vectors/vectors.py:314
msgid "property used for edge_color dropped"
msgstr "ιδιότητα που χρησιμοποιήθηκε για την ακμή_χρώμα έπεσε"

#: napari/layers/points/points.py:919 napari/layers/shapes/shapes.py:931
msgid "_{attribute}_color_property was not set, setting to: {new_color_property}"
msgstr "_{attribute}_color_property δεν ορίστηκε, ρύθμιση σε: {new_color_property}"

#: napari/layers/shapes/shapes.py:940
msgid "There must be a valid Shapes.properties to use {color_mode}"
msgstr "Πρέπει να υπάρχει ένα έγκυρο Shapes.properties για να χρησιμοποιήσετε {color_mode}"

#: napari/layers/points/points.py:941 napari/layers/shapes/shapes.py:953 napari/layers/vectors/vectors.py:526
msgid "selected property must be numeric to use ColorMode.COLORMAP"
msgstr "η επιλεγμένη ιδιότητα πρέπει να είναι αριθμητική για χρήση του ColorMode.COLORMAP"

#: napari/layers/shapes/shapes.py:1325 napari/layers/utils/color_manager_utils.py:47
msgid "face_color should be the name of a color, an array of colors, or the name of an property"
msgstr "face_color θα πρέπει να είναι το όνομα ενός χρώματος, μια σειρά χρωμάτων, ή το όνομα μιας ιδιότητας"

#: napari/layers/shapes/shapes.py:1460
msgid "hold <space> to pan/zoom, press <{BACKSPACE}> to remove selected"
msgstr "κρατήστε πατημένο το <space> για pan/zoom, πατήστε <{BACKSPACE}> για αφαίρεση των επιλεγμένων"

#: napari/layers/points/points.py:1103 napari/layers/points/points.py:1110 napari/layers/shapes/shapes.py:1469 napari/layers/shapes/shapes.py:1478
msgid "hold <space> to pan/zoom"
msgstr "κρατήστε πατημένο το <space> για σμίκρυνση"

#: napari/layers/shapes/shapes.py:1488
msgid "hold <space> to pan/zoom, press <esc> to finish drawing"
msgstr "κρατήστε πατημένο το <space> για μετατόπιση/μεγέθυνση, πατήστε <esc> για να ολοκληρώσετε το σχέδιο"

#: napari/layers/shapes/shapes.py:1865
msgid "the number of properties must equal the number of shapes"
msgstr "ο αριθμός των ιδιοτήτων πρέπει να ισούται με τον αριθμό των σχημάτων"

#: napari/layers/shapes/shapes.py:2276
msgid "expand_shape is deprecated and will be removed in version 0.4.9. It should no longer be used as layers should will soon not know which dimensions are displayed. Instead you should work with full nD shape data as much as possible."
msgstr "expand_shape είναι παρωχημένη και θα αφαιρεθεί στην έκδοση 0.4.9. Δεν θα πρέπει πλέον να χρησιμοποιείται ως στρώματα σύντομα δεν θα γνωρίζει ποιες διαστάσεις εμφανίζονται. Αντ 'αυτού θα πρέπει να εργαστείτε με πλήρη δεδομένα σχήματος nD όσο το δυνατόν περισσότερο."

#: napari/layers/utils/_link_layers.py:74
msgid "Cannot link attributes that are not shared by all layers: {extra}. Allowable attrs include:\n"
"{valid_attrs}"
msgstr "Δεν είναι δυνατός ο σύνδεσμος ιδιοτήτων που δεν μοιράζονται από όλα τα επίπεδα: {extra}. Οι επιτρεπόμενες attrs περιλαμβάνουν:\n"
"{valid_attrs}"

#: napari/layers/utils/_link_layers.py:136
msgid "Must provide at least one layer to unlink"
msgstr "Πρέπει να παρέχει τουλάχιστον ένα επίπεδο για αποσύνδεση"

#: napari/layers/utils/_link_layers.py:194
msgid "``layers`` iterable must have at least one layer"
msgstr "```layers`` iterable πρέπει να έχει τουλάχιστον ένα στρώμα"

#: napari/layers/utils/_text_utils.py:44
msgid "view_data should be a numpy array or list when using Anchor.CENTER"
msgstr "view_data πρέπει να είναι ένας αριθμητικός πίνακας ή λίστα όταν χρησιμοποιείτε το Anchor.CENTER"

#: napari/layers/utils/_text_utils.py:141
msgid "view_data should be a numpy array or list"
msgstr "view_data πρέπει να είναι ένας αριθμητικός πίνακας ή λίστα"

#: napari/layers/utils/_text_utils.py:189
msgid "Number of text elements ({length}) should equal the length of the data ({n_text})"
msgstr "Αριθμός στοιχείων κειμένου ({length}) πρέπει να ισούται με το μήκος των δεδομένων ({n_text})"

#: napari/layers/utils/color_manager.py:66
msgid "color_properties dictionary should have keys: name, values, and optionally current_value"
msgstr "το λεξικό color_properties θα πρέπει να έχει κλειδιά: όνομα, τιμές και προαιρετικά current_value"

#: napari/layers/utils/color_manager.py:76
msgid "color_properties should be None, a dict, or ColorProperties object"
msgstr "color_properties δεν πρέπει να είναι κανείς, ένα αντικείμενο dict, ή ColorProperties"

#: napari/layers/utils/color_manager.py:485
msgid "if color_properties is a string, it should be a property name"
msgstr "αν το color_properties είναι συμβολοσειρά, θα πρέπει να είναι όνομα ιδιότητας"

#: napari/layers/utils/color_transformations.py:45
msgid "The provided {elem_name} parameter contained illegal values, resetting all {elem_name} values to {default}."
msgstr "Η παρεχόμενη παράμετρος {elem_name} περιείχε παράνομες τιμές, επαναφέροντας όλες τις {elem_name} τιμές στο {default}."

#: napari/layers/utils/color_transformations.py:60
msgid "The provided {elem_name} parameter has {length} entries, while the data contains {num_entries} entries. Setting {elem_name} to {default}."
msgstr "Η παρεχόμενη παράμετρος {elem_name} έχει {length} καταχωρήσεις, ενώ τα δεδομένα περιέχουν {num_entries} καταχωρήσεις. Ρύθμιση {elem_name} έως {default}."

#: napari/layers/utils/color_transformations.py:137
msgid "The number of supplied colors mismatch the number of given data points. Length of data is {num_entries}, while the number of colors is {length}. Color for all points is reset to white."
msgstr "Ο αριθμός των παρεχόμενων χρωμάτων δεν ταιριάζει με τον αριθμό των δεδομένων σημεία. Το μήκος των δεδομένων είναι {num_entries}, ενώ ο αριθμός των χρωμάτων είναι {length}. Το χρώμα για όλα τα σημεία επαναφέρεται στο λευκό."

#: napari/layers/utils/stack_utils.py:102
msgid "Error adding multichannel image with data shape {data_shape!r}.\n"
"Requested channel_axis ({channel_axis}) had length {n_channels}, but the '{key}' argument only provided {i} values. "
msgstr "Σφάλμα κατά την προσθήκη εικόνας πολλαπλών καναλιών με το σχήμα δεδομένων {data_shape!r}.\n"
"Το ζητούμενο channel_axis ({channel_axis}) είχε μήκος {n_channels}, αλλά η παράμετρος '{key}' παρείχε μόνο {i} τιμές. "

#: napari/layers/utils/stack_utils.py:159
msgid "The image needs more than 2 dimensions for splitting"
msgstr "Η εικόνα χρειάζεται περισσότερες από 2 διαστάσεις για διαχωρισμό"

#: napari/layers/utils/stack_utils.py:167
msgid "Can't split along axis {axis}. The image has {num_dim} dimensions"
msgstr "Δεν μπορεί να διαχωριστεί κατά μήκος του άξονα {axis}. Η εικόνα έχει {num_dim} διαστάσεις"

#: napari/layers/utils/stack_utils.py:235
msgid "images list is empty"
msgstr "η λίστα εικόνων είναι κενή"

#: napari/layers/utils/text.py:217
msgid "opaque blending mode is not allowed for text. setting to translucent."
msgstr "η αδιαφανής λειτουργία ανάμειξης δεν επιτρέπεται για τη ρύθμιση κειμένου."

#: napari/layers/vectors/_vector_utils.py:35
msgid "Vector data of shape {shape} is not supported"
msgstr "Δεν υποστηρίζονται διανυσματικά δεδομένα σχήματος {shape}"

#: napari/layers/points/points.py:572 napari/layers/vectors/vectors.py:336
msgid "the number of properties must equal the number of points"
msgstr "ο αριθμός των ιδιοτήτων πρέπει να ισούται με τον αριθμό των πόντων"

#: napari/layers/vectors/vectors.py:504
msgid "edge_color property was not set, setting to: {color_property}"
msgstr "edge_color ιδιότητα δεν ορίστηκε, ρύθμιση σε: {color_property}"

#: napari/layers/vectors/vectors.py:513
msgid "There must be a valid Points.properties to use {edge_color_mode}"
msgstr "Πρέπει να υπάρχει μια έγκυρη Points.properties για να χρησιμοποιήσετε {edge_color_mode}"

#: napari/layers/image/_image_utils.py:68
msgid "Input data should be an array-like object, or a sequence of arrays of decreasing size. Got arrays of single shape: {shape}"
msgstr "Τα δεδομένα εισόδου πρέπει να είναι ένα αντικείμενο που μοιάζει με πίνακα ή μια ακολουθία συστοιχιών φθίνοντος μεγέθους. Έχουν πολλές σειρές σχήματος: {shape}"

#: napari/layers/image/_image_utils.py:76
msgid "Input data should be an array-like object, or a sequence of arrays of decreasing size. Got arrays in incorrect order, shapes: {shapes}"
msgstr "Τα δεδομένα εισόδου πρέπει να είναι ένα αντικείμενο που μοιάζει με πίνακα ή μια ακολουθία συστοιχιών μειούμενου μεγέθους. Έχεις πίνακες σε εσφαλμένη σειρά, σχήματα: {shapes}"

#: napari/layers/points/_points_constants.py:84
msgid "arrow"
msgstr "βέλος"

#: napari/layers/points/_points_constants.py:85
msgid "clobber"
msgstr "λόμπυ"

#: napari/layers/points/_points_constants.py:86
msgid "cross"
msgstr "σταυρός"

#: napari/layers/points/_points_constants.py:87
msgid "diamond"
msgstr "διαμάντι"

#: napari/layers/points/_points_constants.py:88
msgid "disc"
msgstr "δίσκος"

#: napari/layers/points/_points_constants.py:89
msgid "hbar"
msgstr "hbar"

#: napari/layers/points/_points_constants.py:90
msgid "ring"
msgstr "δακτύλιος"

#: napari/layers/points/_points_constants.py:92
msgid "star"
msgstr "αστέρι"

#: napari/layers/points/_points_constants.py:93
msgid "tailed arrow"
