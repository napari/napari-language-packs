#: napari/layers/tracks/_track_utils.py:205
msgid "track vertices should be a NxD array"
msgstr "トラック頂点はNxD配列でなければなりません"

#: napari/layers/tracks/_track_utils.py:210
msgid "track vertices should be 4 or 5-dimensional"
msgstr "トラック頂点は4または5次元でなければなりません"

#: napari/layers/tracks/_track_utils.py:220
msgid "track id must be an integer"
msgstr "トラック IDは整数でなければなりません"

#: napari/layers/tracks/_track_utils.py:225
msgid "track timestamps must be greater than zero"
msgstr "トラックタイムスタンプはゼロより大きくなければなりません"

#: napari/layers/tracks/_track_utils.py:234
msgid "tracks should be ordered by ID and time"
msgstr "トラックはIDと時刻順に並び替える必要があります"

#: napari/layers/tracks/_track_utils.py:249
msgid "the number of properties must equal the number of vertices"
msgstr "プロパティの数は頂点の数と等しくなければなりません"

#: napari/layers/tracks/_track_utils.py:277
msgid "graph node {node_idx} not found"
msgstr "グラフノード {node_idx} が見つかりません"

#: napari/layers/tracks/_track_utils.py:345
msgid "Property {color_by} not found"
msgstr "プロパティ {color_by} が見つかりません"

#: napari/layers/tracks/tracks.py:385
msgid "Previous color_by key {key!r} not present in new properties. Falling back to track_id"
msgstr "前の color_by キー {key!r} は新しいプロパティに存在しません。track_id に戻ります"

#: napari/layers/tracks/tracks.py:470
msgid "{color_by} is not a valid property key"
msgstr "{color_by} は有効なプロパティキーではありません"

#: napari/layers/tracks/tracks.py:489
msgid "Colormap {colormap} not available"
msgstr "Colormap {colormap} は利用できません"

#: napari/layers/surface/surface.py:157
msgid "Surface data tuple must be 2 or 3, specifying verictes, faces, and optionally vertex values, instead got length {length}."
msgstr "サーフェスタデータタプルは2または3でなければなりません。頂点、面、および必要に応じて頂点の値は長さ {length}でなければなりません。"

#: napari/layers/surface/surface.py:203
msgid "Surface data tuple must be 2 or 3, specifying vertices, faces, and optionally vertex values, instead got length {data_length}."
msgstr "サーフェスタデータタプルは 2 または 3 でなければなりません。頂点、面、および必要に応じて頂点の値は長さ {data_length} でなければなりません。"

#: napari/layers/surface/surface.py:318
msgid "Assigning multiple values per vertex after slicing is not allowed. All dimensions corresponding to vertex_values must be non-displayed dimensions. Data will not be visible."
msgstr "スライス後に頂点ごとに複数の値を割り当てることは許可されていません。頂点_値に対応するすべての寸法は非表示の寸法でなければなりません。データは表示されません。"

#: napari/layers/shapes/_shape_list.py:152
msgid "{attribute}_color must have shape ({n_shapes}, 4)"
msgstr "{attribute}_color must have shape ({n_shapes}, 4)"

#: napari/layers/shapes/_shape_list.py:248
msgid "shape must be subclass of Shape"
msgstr "シェイプはシェイプのサブクラスでなければなりません"

#: napari/layers/shapes/_shape_list.py:490
msgid "{shape_type} must be one of {shape_classes}"
msgstr "{shape_type} は {shape_classes} のいずれかでなければなりません"

#: napari/layers/shapes/_shapes_utils.py:403
msgid "Data shape does not match expected `[4, D]` shape specifying corners for the rectangle"
msgstr "長方形の角を指定する `[4, D]` の形状とデータの形状が一致しません"

#: napari/layers/shapes/_shapes_utils.py:498
msgid "Data shape does not match expected `[4, D]` shape specifying corners for the ellipse"
msgstr "楕円の角を指定する `[4, D]` 形状のデータ形状が一致しません。"

#: napari/layers/shapes/shapes.py:337
msgid "Shape dimensions must be equal to ndim"
msgstr "シェイプの寸法はndimと等しくなければなりません"

#: napari/layers/points/points.py:329 napari/layers/shapes/shapes.py:407
msgid "text should be a string, array, or dict"
msgstr "テキストは文字列、配列、またはdictでなければなりません"

#: napari/layers/shapes/shapes.py:452 napari/layers/shapes/shapes.py:1454
msgid "enter a selection mode to edit shape properties"
msgstr "図形のプロパティを編集するには、選択モードを入力します。"

#: napari/layers/points/points.py:505 napari/layers/shapes/shapes.py:626
msgid "property used for face_color dropped"
msgstr "face_color のドロップに使用されるプロパティ"

#: napari/layers/points/points.py:524 napari/layers/shapes/shapes.py:638 napari/layers/vectors/vectors.py:314
