#: napari/layers/shapes/_shapes_models/_polgyon_base.py:71
msgid "Shape needs at least two vertices, {number} provided."
msgstr "Muotoon tarvitaan vähintään kaksi verticiä, {number} tarjousta."

#: napari/layers/shapes/_shapes_models/ellipse.py:76
msgid "Data shape does not match a ellipse. Ellipse expects four corner vertices, {number} provided."
msgstr "Datan muoto ei vastaa ellipsiä. Ellipse odottaa neljän kulman vertices, {number} tarjousta."

#: napari/layers/shapes/_shapes_models/line.py:61
msgid "Data shape does not match a line. A line expects two end vertices, {number} provided."
msgstr "Datan muoto ei vastaa riviä. Linja odottaa kahta loppuvertikaalia, {number} tarjousta."

#: napari/layers/shapes/_shapes_models/rectangle.py:68
msgid "Data shape does not match a rectangle. Rectangle expects four corner vertices, {number} provided."
msgstr "Datan muoto ei vastaa suorakulmiota. Suorakulmio odottaa neljän kulman vertices, {number} tarjousta."

#: napari/layers/shapes/_shapes_models/shape.py:346
msgid "Axis not recognized, must be one of \"{{0, 1}}\""
msgstr "Akselia ei tunnistettu, täytyy olla yksi \"{{0, 1}}\""

#: napari/layers/shapes/_shapes_models/shape.py:400
msgid "mask shape length must either be 2 or the same as the dimensionality of the shape, expected {expected} got {received}."
msgstr "maskin muodon pituuden on oltava joko 2 tai sama kuin muodon mitta, odotettu {expected} sai {received}."

#: napari/layers/image/experimental/_octree_slice.py:92
msgid "Octree level {index} is not in range(0, {num_levels})"
msgstr "Octree taso {index} ei ole alueella (0, {num_levels})"

#: napari/layers/image/experimental/_octree_slice.py:137
msgid "Invalid octree level {index}"
msgstr "Virheellinen octree taso {index}"

#: napari/layers/image/experimental/octree.py:66
msgid "Data of shape {shape} resulted no octree levels?"
msgstr "Muodon {shape} tiedot eivät johtaneet octree tasoihin?"

#: napari/layers/image/experimental/octree.py:116
msgid "Level {level_index} is not in range(0, {top})"
msgstr "Taso {level_index} ei ole alueella (0, {top})"

#: napari/layers/image/experimental/octree.py:403
msgid "Multiscale data has downsampling ratio of {ratio}, expected 2."
msgstr "Multiscale tiedot on laskenta suhde {ratio}, odotetaan 2."

#: napari/layers/image/experimental/octree_image.py:223
msgid "Octree level {level} is negative."
msgstr "Octree taso {level} on negatiivinen."

